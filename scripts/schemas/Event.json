    "properties": {
      "name": {
        "type": "string",
        "maxLength": 240
      },
      "icon": {
        "title": "icon",
        "type": "string"
      },
      "desc": {
        "title": "desc",
        "type": "string"
      },
      "url": {
        "title": "url",
        "type": "string"
      },
      "organizationId": {
        "type": "string",
        "maxLength": 240
      }
    },
