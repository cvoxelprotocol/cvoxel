{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
    "title": "Event",
    "properties": {
      "name": {
        "type": "string",
        "maxLength": 240
      },
      "icon": {
        "title": "icon",
        "type": "string"
      },
      "desc": {
        "title": "desc",
        "type": "string"
      },
      "url": {
        "title": "url",
        "type": "string"
      },
      "organizationId": {
        "type": "string",
        "maxLength": 240
      },
      "tags": {
        "type": "array",
        "title": "tags",
        "description": "tags",
        "items": {
          "type": "string"
        },
        "uniqueItems": true
      },
      "startDate": {
        "type": "string",
        "title": "startDate",
        "description": "Time stamp of event started"
      },
      "endDate": {
        "type": "string",
        "title": "endDate",
        "description": "Time stamp of event ended"
      }
    },
    "required": ["name"],
    "additionalProperties": true
}